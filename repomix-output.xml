redisPub.on('connect', () => console.log('‚úÖ Connected to Redis (pub) at', REDIS_URL));
redisPub.on('error', err => console.error('‚ùå Redis pub error:', err));
redisSub.on('connect', () => console.log('‚úÖ Connected to Redis (sub) at', REDIS_URL));
redisSub.on('error', err => console.error('‚ùå Redis sub error:', err));

// basic health endpoint
